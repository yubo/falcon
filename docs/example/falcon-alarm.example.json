{
    "debug": true,
    "http": {
        "enabled": true,
        "listen": "%%ALARM_HTTP%%"
    },
    "queue": {
        "sms": "/sms",
        "mail": "/mail"
    },
    "database": {
        "addr": "%%DB_ALARM%%?loc=Local&parseTime=true",
        "idle": 2
    },
    "redis": {
        "addr": "%%REDIS_HTTP%%",
        "maxIdle": 5,
        "highQueues": [
            "event:p0",
            "event:p1"
        ],
        "lowQueues": [
            "event:p2",
            "event:p3",
            "event:p4",
            "event:p5",
            "event:p6"
        ],
        "userSmsQueue": "/queue/user/sms",
        "userMailQueue": "/queue/user/mail"
    },
    "api": {
        "xpharos": "http://%%XPHAROS_HTTP%%/v1/monitor/api/conf/id/%s",
        "ctrl": "http://%%CTRL_HTTP%%",
        "slinker": "http://%%SLINKER_HTTP%%",
        "portal": "http://[%%PORTAL_HTTP%%]",
        "hostnames": "http://%%NORNS_HTTP%%/api/v1/tagstring/%s/hostnames",
        "links": "http://%%LINKS_HTTP%%",
        "linker":"http://%%LINKER_HTTP%%"
    },
    "statistic": {
        "interval": 60,
        "pushAddr": "http://%%AGENT_HTTP%%/v1/push"
    }
}
