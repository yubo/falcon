{
    "debug": true,
    "http": {
        "enabled": true,
        "listen": "%%NODATA_HTTP%%"
    },
    "query":{
        "connectTimeout": 5000,
        "requestTimeout": 30000,
        "queryAddr": "%%TRANSFER_HTTP%%"
    },
    "config": {
        "enabled": true,
        "dsn": "%%DB_FALCON%%?loc=Local&parseTime=true&wait_timeout=604800",
        "maxIdle": 4
    },
    "collector":{
        "enabled": true,
        "batch": 200,
        "concurrent": 10
    },
    "sender":{
        "enabled": true,
        "connectTimeout": 5000,
        "requestTimeout": 30000,
        "transferAddr": "%%TRANSFER_HTTP%%",
        "batch": 500,
        "block": {
            "enabled": true,
            "threshold": 32,
            "setBlock": false,
            "enableGauss": false,
            "hostname": "",
            "floodCounter": "",
            "gaussFilter": 1,
            "gauss3SigmaMin": 2,
            "gauss3SigmaMax": 5
        }
    },
    "api":{
        "ctrl": "http://%%CTRL_HTTP%%"
    }
}
