{
	"debug": true,
	"hostname": "",
    "safeguard": {
        "maxMemMByte": 5000
    },
	"plugin": {
		"enabled": true,
		"dir": "./plugin",
		"tarball": "http://%%PLUGIN_HTTP%%/plugin.tar.gz",
		"logs": "./logs"
	},
	"heartbeat": {
		"enabled": true,
		"addr": "%%HBS_RPC%%",
		"interval": 60,
		"timeout": 1000
	},
	"transfer": {
		"enabled": true,
		"isGrpcBackend": false,
		"addr": "%%TRANSFER_RPC%%",
		"interval": 60,
		"timeout": 1000,
        "batch": 200
	},
	"http": {
		"enabled": true,
		"listen": "%%AGENT_HTTP%%"
	},
	"collector": {
		"ifacePrefix": ["eth", "em"]
	}
}
