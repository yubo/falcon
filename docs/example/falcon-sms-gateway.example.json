{    
    "debug":false,
    "admin":"",
    "provider":"mi",
    "threshold":1000,
    "cron": {
        "resend": {
            "enabled":false,
            "interval":60
        },
        "checkReport":{
            "enabled":false,
            "interval":60
        },
        "statistic":{
            "interval":60,
            "pushAddr":"http://127.0.0.1:%%AGENT_HTTP%%/v1/push"
        }
    },
    "http": {
        "enabled": true,
        "listen": "%%SMS_HTTP%%",
        "secret": ""
    },
    "log": {
        "access": "var/access.log",
        "output": "var/app.log",
        "error": "var/error.log",
        "success": "var/success.log",
        "show_sql": true
    },
    "mysql":{
        "addr": "%%DB_SMS_GATEWAY%%?charset=utf8&&loc=Asia%2FShanghai",
        "idle": 10,
        "max": 200
    },
    "sms":{
        "addr": {
            "mi":"http://%%SMS_MI%%/sender/sms",
            "other":"http://%%SMS_OTHER%%/sms"
        },
        "account":"%%SMS_ACCOUNT%%",
        "password":"%%SMS_PASS%%",
        "extno":"%%SMS_EXTNO%%"
    }
}
