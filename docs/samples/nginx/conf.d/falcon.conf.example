server {
	listen		7000;
	#server_name	c3-op-mon-dev01.bj;
	#access_log	off;
	root		/opt/falcon/var/html;


	location ^~ /v1.0 {
		proxy_pass http://127.0.0.1:18005;
	}

	location ^~ /docs {
		index index.html index.htm index.md;
		autoindex on;
	}

	location / {
		#proxy_pass http://localhost:3000;
		try_files $uri /index.html;
	}
}
